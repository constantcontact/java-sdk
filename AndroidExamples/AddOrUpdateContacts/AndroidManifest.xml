<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.constantcontact.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.constantcontact.demo.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.constantcontact.demo.SearchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.constantcontact.demo.ContactDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.constantcontact.demo.ContactListsActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.constantcontact.demo.ContactAddressActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.constantcontact.demo.ContactCustomFieldsActivity"
            android:screenOrientation="portrait" >
        </activity>
    </application>
</manifest>